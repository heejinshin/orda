{
  "swagger": "2.0",
  "info": {
    "title": "Orda gRPC gateway APIs",
    "version": "v1"
  },
  "tags": [
    {
      "name": "OrdaService"
    }
  ],
  "consumes": [
    "application/json"
  ],
  "produces": [
    "application/json"
  ],
  "paths": {
    "/api/v1/collections/{collection}": {
      "put": {
        "operationId": "OrdaService_CreateCollection",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/ordaCollectionMessage"
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/rpcStatus"
            }
          }
        },
        "parameters": [
          {
            "name": "collection",
            "in": "path",
            "required": true,
            "type": "string"
          }
        ],
        "tags": [
          "OrdaService"
        ]
      }
    },
    "/api/v1/collections/{collection}/clients/{cuid}": {
      "post": {
        "operationId": "OrdaService_ProcessClient",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/ordaClientMessage"
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/rpcStatus"
            }
          }
        },
        "parameters": [
          {
            "name": "collection",
            "in": "path",
            "required": true,
            "type": "string"
          },
          {
            "name": "cuid",
            "in": "path",
            "required": true,
            "type": "string"
          },
          {
            "name": "body",
            "in": "body",
            "required": true,
            "schema": {
              "$ref": "#/definitions/ordaClientMessage"
            }
          }
        ],
        "tags": [
          "OrdaService"
        ]
      }
    },
    "/api/v1/collections/{collection}/datatypes/{key}": {
      "get": {
        "operationId": "OrdaService_SnapshotDatatype",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/ordaSnapshotResponse"
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/rpcStatus"
            }
          }
        },
        "parameters": [
          {
            "name": "collection",
            "in": "path",
            "required": true,
            "type": "string"
          },
          {
            "name": "key",
            "in": "path",
            "required": true,
            "type": "string"
          },
          {
            "name": "duid",
            "in": "query",
            "required": false,
            "type": "string"
          },
          {
            "name": "sseq",
            "in": "query",
            "required": false,
            "type": "string",
            "format": "uint64"
          }
        ],
        "tags": [
          "OrdaService"
        ]
      }
    },
    "/api/v1/collections/{collection}/pushpulls/{cuid}": {
      "post": {
        "operationId": "OrdaService_ProcessPushPull",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/ordaPushPullMessage"
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/rpcStatus"
            }
          }
        },
        "parameters": [
          {
            "name": "collection",
            "in": "path",
            "required": true,
            "type": "string"
          },
          {
            "name": "cuid",
            "in": "path",
            "required": true,
            "type": "string"
          },
          {
            "name": "body",
            "in": "body",
            "required": true,
            "schema": {
              "$ref": "#/definitions/ordaPushPullMessage"
            }
          }
        ],
        "tags": [
          "OrdaService"
        ]
      }
    },
    "/api/v1/collections/{collection}/reset": {
      "put": {
        "operationId": "OrdaService_ResetCollection",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/ordaCollectionMessage"
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/rpcStatus"
            }
          }
        },
        "parameters": [
          {
            "name": "collection",
            "in": "path",
            "required": true,
            "type": "string"
          }
        ],
        "tags": [
          "OrdaService"
        ]
      }
    },
    "/api/v1/samples/operation": {
      "post": {
        "operationId": "OrdaService_TestEncodingOperation",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/ordaEncodingMessage"
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/rpcStatus"
            }
          }
        },
        "parameters": [
          {
            "name": "body",
            "in": "body",
            "required": true,
            "schema": {
              "$ref": "#/definitions/ordaEncodingMessage"
            }
          }
        ],
        "tags": [
          "OrdaService"
        ]
      }
    }
  },
  "definitions": {
    "ordaCheckPoint": {
      "type": "object",
      "properties": {
        "sseq": {
          "type": "string",
          "format": "uint64",
          "title": "@inject_tag: bson:\"s\",json:\"s\""
        },
        "cseq": {
          "type": "string",
          "format": "uint64",
          "title": "@inject_tag: bson:\"c\",json:\"c\""
        }
      }
    },
    "ordaClientMessage": {
      "type": "object",
      "properties": {
        "header": {
          "$ref": "#/definitions/ordaHeader"
        },
        "collection": {
          "type": "string"
        },
        "cuid": {
          "type": "string"
        },
        "clientAlias": {
          "type": "string"
        },
        "clientType": {
          "$ref": "#/definitions/ordaClientType"
        },
        "syncType": {
          "$ref": "#/definitions/ordaSyncType"
        }
      }
    },
    "ordaClientType": {
      "type": "string",
      "enum": [
        "PERSISTENT",
        "EPHEMERAL"
      ],
      "default": "PERSISTENT"
    },
    "ordaCollectionMessage": {
      "type": "object",
      "properties": {
        "collection": {
          "type": "string"
        }
      }
    },
    "ordaDatatypeMeta": {
      "type": "object",
      "properties": {
        "key": {
          "type": "string"
        },
        "DUID": {
          "type": "string"
        },
        "opID": {
          "$ref": "#/definitions/ordaOperationID"
        },
        "typeOf": {
          "$ref": "#/definitions/ordaTypeOfDatatype"
        }
      }
    },
    "ordaEncodingMessage": {
      "type": "object",
      "properties": {
        "type": {
          "$ref": "#/definitions/ordaTypeOfDatatype"
        },
        "op": {
          "$ref": "#/definitions/ordaOperation"
        }
      }
    },
    "ordaHeader": {
      "type": "object",
      "properties": {
        "version": {
          "type": "string"
        },
        "agent": {
          "type": "string"
        },
        "type": {
          "$ref": "#/definitions/ordaRequestType"
        }
      }
    },
    "ordaOperation": {
      "type": "object",
      "properties": {
        "ID": {
          "$ref": "#/definitions/ordaOperationID"
        },
        "opType": {
          "$ref": "#/definitions/ordaTypeOfOperation"
        },
        "body": {
          "type": "string",
          "format": "byte"
        }
      }
    },
    "ordaOperationID": {
      "type": "object",
      "properties": {
        "era": {
          "type": "integer",
          "format": "int64",
          "title": "@inject_tag: json:\"e,omitempty\""
        },
        "lamport": {
          "type": "string",
          "format": "uint64",
          "title": "@inject_tag: json:\"l,omitempty\""
        },
        "CUID": {
          "type": "string",
          "title": "@inject_tag: json:\"c,omitempty\""
        },
        "seq": {
          "type": "string",
          "format": "uint64",
          "title": "@inject_tag: json:\"s,omitempty\""
        }
      }
    },
    "ordaPushPullMessage": {
      "type": "object",
      "properties": {
        "header": {
          "$ref": "#/definitions/ordaHeader"
        },
        "collection": {
          "type": "string"
        },
        "cuid": {
          "type": "string"
        },
        "PushPullPacks": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/ordaPushPullPack"
          }
        }
      }
    },
    "ordaPushPullPack": {
      "type": "object",
      "properties": {
        "DUID": {
          "type": "string"
        },
        "key": {
          "type": "string"
        },
        "option": {
          "type": "integer",
          "format": "int64"
        },
        "checkPoint": {
          "$ref": "#/definitions/ordaCheckPoint"
        },
        "era": {
          "type": "integer",
          "format": "int64"
        },
        "type": {
          "$ref": "#/definitions/ordaTypeOfDatatype"
        },
        "operations": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/ordaOperation"
          }
        }
      }
    },
    "ordaRequestType": {
      "type": "string",
      "enum": [
        "CLIENTS",
        "PUSHPULLS"
      ],
      "default": "CLIENTS"
    },
    "ordaSnapshotResponse": {
      "type": "object",
      "properties": {
        "meta": {
          "$ref": "#/definitions/ordaDatatypeMeta"
        },
        "json": {
          "type": "string"
        }
      }
    },
    "ordaSyncType": {
      "type": "string",
      "enum": [
        "LOCAL_ONLY",
        "MANUALLY",
        "REALTIME"
      ],
      "default": "LOCAL_ONLY"
    },
    "ordaTypeOfDatatype": {
      "type": "string",
      "enum": [
        "COUNTER",
        "MAP",
        "LIST",
        "DOCUMENT"
      ],
      "default": "COUNTER"
    },
    "ordaTypeOfOperation": {
      "type": "string",
      "enum": [
        "NO_OP",
        "SNAPSHOT",
        "ERROR",
        "TRANSACTION",
        "COUNTER_INCREASE",
        "MAP_PUT",
        "MAP_REMOVE",
        "LIST_INSERT",
        "LIST_DELETE",
        "LIST_UPDATE",
        "DOC_OBJ_PUT",
        "DOC_OBJ_RMV",
        "DOC_ARR_INS",
        "DOC_ARR_DEL",
        "DOC_ARR_UPD"
      ],
      "default": "NO_OP"
    },
    "protobufAny": {
      "type": "object",
      "properties": {
        "typeUrl": {
          "type": "string"
        },
        "value": {
          "type": "string",
          "format": "byte"
        }
      }
    },
    "rpcStatus": {
      "type": "object",
      "properties": {
        "code": {
          "type": "integer",
          "format": "int32"
        },
        "message": {
          "type": "string"
        },
        "details": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/protobufAny"
          }
        }
      }
    }
  }
}
